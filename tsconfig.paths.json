{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@helpers": ["src/helpers"],
      "@helpers/*": ["src/helpers/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@styles": ["src/styles"],
      "@styles/*": ["src/styles/*"]
    }
  }
}
